class Solution:
    def isPalindrome(self, s):
       
        # s = S.lower().replace(" ", "")
        
        i = 0
        j = len(s) - 1
        c = 0
        
        while i < j: 
            if s[i] != s[j]:
                c += 1
            i += 1
            j -= 1
        
        if c <= 1: 
            return 1
        else:
            return 0
