class Solution:
    def subarrayXor(self, arr, k):
        freq = {}
        xor = 0
        count = 0

        freq[0] = 1 

        for num in arr:
            xor ^= num
            target = xor ^ k

            if target in freq:
                count += freq[target]

            freq[xor] = freq.get(xor, 0) + 1

        return count
